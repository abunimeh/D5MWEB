'architecture behavioral of FrameMask is',
'    signal d1Rgb     : channel;',
'begin',
'SyncFrames32Inst: SyncFrames',
'generic map(',
'    pixelDelay =&gt; 31) --LATENCY 32',
'port map(',
'    clk        =&gt; clk,',
'    reset      =&gt; reset,',
'    iRgb       =&gt; i2Rgb,',

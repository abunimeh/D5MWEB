'                oRgb.blue  &lt;= d1Rgb.blue;',
'            end if;',
'                oRgb.valid &lt;= i1Rgb.valid;',
'        end if;',
'    end process;',
'end generate EBLACK_DISABLED;',
'end behavioral;'
];

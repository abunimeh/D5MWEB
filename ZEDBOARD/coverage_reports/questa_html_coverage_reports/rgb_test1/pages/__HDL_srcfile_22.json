'                oRgb.green &lt;= black;',
'                oRgb.blue  &lt;= black;',
'            else',
'                oRgb.red   &lt;= d1Rgb.red;',
'                oRgb.green &lt;= d1Rgb.green;',
'                oRgb.blue  &lt;= d1Rgb.blue;',
'            end if;',
'                oRgb.valid &lt;= i1Rgb.valid;',
'        end if;',
'    end process;',
'end generate EBLACK_ENABLED;',
